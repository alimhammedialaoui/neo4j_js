r.close()\r\n    }\r\n\r\n\r\n\r\n    \r\n\r\n\r\n    addMoyenDeTransport = async (moyenDeTransport) => {\r\n        try {\r\n            const result = await session.run(\r\n                'CREATE (a:MoyenDeTransport {type: $type,numero: $numero,constructeur: $constructeur, anneemiseenservice: $anneemiseenservice}) RETURN a',\r\n                {\r\n                    type: moyenDeTransport.type,\r\n                    numero: moyenDeTransport.numero,\r\n                    constructeur: moyenDeTransport.constructeur,\r\n                    anneemiseenservice: moyenDeTransport.anneemiseenservice\r\n\r\n                }\r\n            )\r\n\r\n            const singleRecord = result.records[0]\r\n            const node = singleRecord.get(0)\r\n\r\n            console.log(node.properties.name)\r\n        } finally {\r\n            await session.close()\r\n        }\r\n\r\n// on application exit:\r\n        await driver.close()\r\n    }\r\n\r\n}\r\n\r\nexport default new Api();\r\n\r\n","C:\\Users\\alima\\Desktop\\Benhlima\\neo4j_js.git\\src\\component\\UserPreview.js",[],"C:\\Users\\alima\\Desktop\\Benhlima\\neo4j_js.git\\src\\component\\FormMoyenDeTransport.js",[],"C:\\Users\\alima\\Desktop\\Benhlima\\neo4j_js.git\\src\\component\\AddPreferenceTransport.js",["70"],"C:\\Users\\alima\\Desktop\\Benhlima\\neo4j_js.git\\src\\component\\ShortestPath.js",[],"C:\\Users\\alima\\Desktop\\Benhlima\\neo4j_js.git\\src\\component\\MoyenDeTransportPreview.js",["71","72","73"],"import React, {Component} from 'react';\r\nimport Api from \"../api/call\"\r\nimport FormMoyenDeTransport from \"./FormMoyenDeTransport\";\r\nimport MoyenDeTransport from \"./MoyenDeTransport\";\r\nimport AddPreferenceTransport from \"./AddPreferenceTransport\";\r\n\r\n\r\nclass MoyenDeTransportPreview extends Component {\r\n    \r\n        state = {\r\n            moyens:[],\r\n            showAddForm: false,\r\n            showPrefForm: false,\r\n            showUpdForm: false,\r\n            nom: \"\"\